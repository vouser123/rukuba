{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "PT Rebuild Dev Notes",
  "type": "object",
  "required": [
    "metadata",
    "enums",
    "open_items",
    "closed_items"
  ],
  "properties": {
    "metadata": {
      "type": "object",
      "required": [
        "schema_version",
        "last_updated"
      ],
      "properties": {
        "schema_version": {
          "type": "string"
        },
        "last_updated": {
          "type": "string"
        }
      }
    },
    "enums": {
      "type": "object",
      "required": [
        "priority_levels",
        "risk_levels",
        "status_values",
        "tag_vocabulary"
      ]
    },
    "open_items": {
      "type": "array"
    },
    "closed_items": {
      "type": "array"
    }
  }
}
